<!DOCTYPE html>
<html>
<head>
  <title>TeamD</title>
  <%= stylesheet_link_tag    "application", media: "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  		<meta charset="utf-8">
		<script type="text/javascript" src="./html5jp/excanvas/excanvas.js"></script>
		<script type="text/javascript" src="./html5jp/graph/line.js"></script>
		<script type="text/javascript" src="./script.js"></script>
    <script type="text/css" src="./common.css"></script>
    <script type="text/css" src="./reset.css"></script>
		<script type="text/javascript">
		window.onload = function() {
		  var lg = new html5jp.graph.line("footmark");
		  if( ! lg ) { return; }
		  var items = [
		    ["足跡の数", 20, 58, 40, 14, 38, 20, 80],
		  ];
		  var params = {
		    x: ["日時", "日", "月", "火", "水", "木", "金", "土"],//日付
		    y: ["View数", 0, 20, 40, 60, 80, 100], 
		    yMax: 100,
		    yMin: 0,
		    lineWidth: [1,2],
		    dotRadius: [3,4],
		    dotType: ["disc", "square"]
		  };
		  lg.draw(items, params);
		};
		</script>
</head>
<body>

<%= yield %>

</body>
</html>
